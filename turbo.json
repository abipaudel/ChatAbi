{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"],
      "inputs": ["**/*.tsx", "**/*.ts", "**/*.js"],
      "outputMode": "new-only"
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    }
  }
}
